<?xml version="1.0"?>

<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">

    <!-- /rhq/resource/navigation/ResourcesTree.xhtml -->

    <managed-bean>
        <managed-bean-name>ResourceTreeModelUIBean</managed-bean-name>
        <managed-bean-class>org.rhq.enterprise.gui.navigation.resource.ResourceTreeModelUIBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ResourceTreeContextMenuUIBean</managed-bean-name>
        <managed-bean-class>org.rhq.enterprise.gui.navigation.resource.ResourceTreeContextMenuUIBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>AutoGroupTreeContextMenuUIBean</managed-bean-name>
        <managed-bean-class>org.rhq.enterprise.gui.navigation.resource.AutoGroupTreeContextMenuUIBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ResourceTreeStateAdvisor</managed-bean-name>
        <managed-bean-class>org.rhq.enterprise.gui.navigation.resource.ResourceTreeStateAdvisor</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <!-- /rhq/resource/navigation/GroupTree.xhtml -->

    <managed-bean>
       <managed-bean-name>ResourceGroupTreeModelUIBean</managed-bean-name>
       <managed-bean-class>org.rhq.enterprise.gui.navigation.group.ResourceGroupTreeModelUIBean</managed-bean-class>
       <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ResourceGroupTreeContextMenuUIBean</managed-bean-name>
        <managed-bean-class>org.rhq.enterprise.gui.navigation.group.ResourceGroupTreeContextMenuUIBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ResourceGroupTreeStateAdvisor</managed-bean-name>
        <managed-bean-class>org.rhq.enterprise.gui.navigation.group.ResourceGroupTreeStateAdvisor</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <!-- /rhq/common/menu/menu.xhtml -->

    <managed-bean>
        <managed-bean-name>ResourceSelectUIBean</managed-bean-name>
        <managed-bean-class>org.rhq.enterprise.gui.navigation.resource.ResourceSelectUIBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ResourceGroupSelectUIBean</managed-bean-name>
        <managed-bean-class>org.rhq.enterprise.gui.navigation.group.ResourceGroupSelectUIBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

</faces-config>
