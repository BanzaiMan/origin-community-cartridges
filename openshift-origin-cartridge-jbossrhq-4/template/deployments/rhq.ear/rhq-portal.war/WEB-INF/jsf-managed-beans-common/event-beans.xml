<?xml version="1.0"?>

<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">

   <!-- /rhq/common/monitor/graphs/events/** -->

   <managed-bean>
      <managed-bean-name>EventsTimelineUIBean</managed-bean-name>
      <managed-bean-class>org.rhq.enterprise.gui.measurement.graphs.EventsTimelineUIBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
   </managed-bean>
   
   <!-- /rhq/common/events/history.xhtml -->
   
   <managed-bean>
      <managed-bean-name>EventHistoryUIBean</managed-bean-name>
      <managed-bean-class>org.rhq.enterprise.gui.event.EventHistoryUIBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
   </managed-bean>
     

</faces-config>
      