<?xml version="1.0"?>

<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">

   <!-- /rhq/definition/group/** -->

   <managed-bean>
      <managed-bean-name>GroupDefinitionUIBean</managed-bean-name>
      <managed-bean-class>org.rhq.enterprise.gui.definition.group.GroupDefinitionUIBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
   </managed-bean>

   <!-- /rhq/definition/group/new.xhtml --> 
   <managed-bean>
      <managed-bean-name>NewGroupDefinitionGeneralPropertiesUIBean</managed-bean-name>
      <managed-bean-class>org.rhq.enterprise.gui.definition.group.NewGroupDefinitionGeneralPropertiesUIBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
   </managed-bean>
   
   <!-- /rhq/definition/group/list.xhtml --> 
   <managed-bean>
      <managed-bean-name>ListGroupDefinitionsUIBean</managed-bean-name>
      <managed-bean-class>org.rhq.enterprise.gui.definition.group.ListGroupDefinitionsUIBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
   </managed-bean>

   <!-- /rhq/definition/group/edit-general.xhtml --> 
   <managed-bean>
      <managed-bean-name>EditGroupDefinitionGeneralPropertiesUIBean</managed-bean-name>
      <managed-bean-class>org.rhq.enterprise.gui.definition.group.EditGroupDefinitionGeneralPropertiesUIBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
   </managed-bean>

   <!-- /rhq/definition/group/library.xhtml -->
   <managed-bean>
      <managed-bean-name>GroupDefinitionExpressionBuilderLibraryUIBean</managed-bean-name>
      <managed-bean-class>org.rhq.enterprise.gui.definition.group.GroupDefinitionExpressionBuilderLibraryUIBean</managed-bean-class>
      <managed-bean-scope>session</managed-bean-scope>
   </managed-bean>

   <managed-bean>
      <managed-bean-name>GroupDefinitionLibraryUIBean</managed-bean-name>
      <managed-bean-class>org.rhq.enterprise.gui.definition.group.GroupDefinitionLibraryUIBean</managed-bean-class>
      <managed-bean-scope>application</managed-bean-scope>
   </managed-bean>

</faces-config>
      